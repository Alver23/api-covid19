{
  "version": 2,
  "name": "api-covid19",
  "env": {
    "NODE_ENV": "production",
    "APP_PORT": 3000,
    "APP_GOV_BASE_URL": "https://www.datos.gov.co/",
    "APP_GOV_PATH": "resource/gt2j-8ykr.json",
    "APP_GOV_TOKEN": "@app-gov-token"
  },
  "builds": [
    {
      "src": "dist/main.js",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/main.js"
    }
  ]
}
